    "lint": "lint-staged --config .config/lint-staged.config.cjs",
    "minor": "npm run build && npm version minor --force && npm publish",
    "patch": "npm run build && npm version patch --force && npm publish",
    "push": "sh ./.config/scripts/push.sh"
