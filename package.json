		"clean": "node -r esm ./.config/scripts/nukeDistFiles.js",
		"install:all": "sh ./.config/scripts/install.sh && rm -rf .config/husky && husky install .config/husky && husky add .config/husky/pre-commit \"npm run lint\"",
		"install:clean": "sh ./.config/scripts/clean-install.sh && rm -rf .config/husky && husky install .config/husky && husky add .config/husky/pre-commit \"npm run lint\"",
		"test": "vitest --config=.config/vite.config.ts",
		"lint": "lint-staged --config .config/lint-staged.config.cjs",
		"minor": "npm run minor:pre && npm publish",
		"minor:pre": "npm run build && npm version minor",
		"patch": "npm run patch:pre && npm publish",
		"patch:pre": "npm run build && npm version patch",
		"push": "sh ./.config/scripts/push.sh",
		"postinstall": "rm -rf .config/husky && husky install .config/husky && husky add .config/husky/pre-commit \"npm run lint\""
	},
